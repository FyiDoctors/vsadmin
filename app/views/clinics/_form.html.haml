= form_for @clinic do |f|
  - if @clinic.errors.any?
    #error_explanation
      %h2= "#{pluralize(@clinic.errors.count, "error")} prohibited this clinic from being saved:"
      %ul
        - @clinic.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :address
    = f.text_field :address
  .field
    = f.label :model_id
    = f.text_field :model_id
  .field
    = f.label :admin
    = f.check_box :admin
  .field
    = f.label :email
    = f.text_field :email
  .field
    = f.label :vendorid
    = f.text_field :vendorid
  .field
    = f.label :vsid
    = f.text_field :vsid
  .field
    = f.label :vsvendorid
    = f.text_field :vsvendorid
  .field
    = f.label :province
    = f.text_field :province
  .actions
    = f.submit 'Save'
